<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.javier.trabajofinalne">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ListadoClientes" />
        <activity android:name=".ModificarCliente"
            android:windowSoftInputMode="stateHidden"/>
        <activity android:name=".FormularioCliente"
            android:windowSoftInputMode="stateHidden"/>
        <activity android:name=".ListadoTipoClientes" />
        <activity android:name=".FormularioTipoCliente"
            android:windowSoftInputMode="stateHidden" />
        <activity android:name=".ModificarTipoCliente"
            android:windowSoftInputMode="stateHidden" />
        <activity android:name=".ListadoZonas" />
        <activity android:name=".FormularioZona"
            android:windowSoftInputMode="stateHidden" />
        <activity android:name=".ModificarZona"
            android:windowSoftInputMode="stateHidden" ></activity>
    </application>

</manifest>